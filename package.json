{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:fix": "next lint --fix",
    "lint": "next lint",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "format": "yarn lint:fix && yarn type:check && yarn prettier:fix",
    "ci:lint": "yarn prettier:check && yarn lint && yarn type:check",
    "type:check": "tsc --noEmit --incremental"
  },
  "dependencies": {
    "jotai": "^2.8.3",
    "jotai-effect": "^1.0.0",
    "lodash": "^4.17.21",
    "next": "14.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "tailwindcss": "^3.4.4",
    "typescript": "5.4.5"
  },
  "packageManager": "yarn@4.3.1"
}
